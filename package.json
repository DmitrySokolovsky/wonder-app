{
  "name": "WonderApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "build-js": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build-apk": "cd android && gradlew clean && gradlew assembleRelease",
    "release": "react-native run-android --variant=release"
  },
  "dependencies": {
    "react": "16.4.1",
    "react-native": "0.55.2",
    "react-native-elements": "^0.19.1",
    "react-native-firebase": "^4.3.8",
    "react-native-indicators": "^0.13.0",
    "react-native-material-textfield": "^0.12.0",
    "react-native-micro-animated-button": "0.0.27",
    "react-native-push-notification": "^3.1.1",
    "react-native-vector-icons": "^5.0.0",
    "react-navigation": "^2.11.2"
  },
  "devDependencies": {
    "babel-jest": "23.4.2",
    "babel-preset-react-native": "4",
    "jest": "23.5.0",
    "react-test-renderer": "16.4.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
